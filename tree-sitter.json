{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "lark",
      "camelcase": "Lark",
      "title": "Lark",
      "scope": "source.lark",
      "file-types": [
        "lark"
      ],
      "highlights": "queries/lark/highlights.scm",
      "locals": "queries/lark/locals.scm",
      "injections": "queries/lark/injections.scm",
      "injection-regex": "^lark$",
      "class-name": "TreeSitterLark"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "Lark grammar for tree-sitter",
    "authors": [
      {
        "name": "chanicpanic"
      }
    ],
    "links": {
      "repository": "https://github.com/chanicpanic/tree-sitter-lark"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}
